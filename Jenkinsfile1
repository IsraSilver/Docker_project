pipeline {
    agent ("slave01")

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('SCM') {
            steps {
                checkout scm
            }
        }
        stage('Build Docker file') {
            steps {
                sh 'docker build -t alpcon:0.0.1 .'
            }
        }
        
        
    }
}
